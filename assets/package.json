  },
  "dependencies": {
    "core-js": "^2.6.9",
    "phoenix": "file:../deps/phoenix",
    "phoenix_html": "file:../deps/phoenix_html"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0",
    "css-loader": "^2.1.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.12.0",
    "parcel": "^2.0.0-alpha.1.1",
    "parcel-plugin-stdin": "^0.1.1",
    "parcel-plugin-bundle-manifest": "^0.2.0",
    "sass": "^1.22.12"
  },
  "browser": "../priv/static",
  "targets": {
    "browser": {
      "publicUrl": "./",
      "engines": {
        "browsers": [
          "> 1%",
          "not dead"
        ]
      }
    }
  }
